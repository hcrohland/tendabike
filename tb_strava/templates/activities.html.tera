{% extends "base" %}

{% block content %}

    <h1>Here are your activities:</h3>
    <ul>
        {% for s in activities %}
            <li>{{ s.name }} at {{ s.start_date_local }} using gear <a href="/user/gear/{{ s.gear_id }}"> {{ s.gear_id }} </a></li>
        {% endfor %}
    </ul>

    <p>Try going to <a href="/user">Your User Page</a></p>
{% endblock content %}